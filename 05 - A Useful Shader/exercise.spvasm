     OpCapability Shader
     OpMemoryModel Logical GLSL450
     OpEntryPoint Vertex %3 "main" %7

     OpName %7 "fancy_attribute"

; the `float` type
%5 = OpTypeFloat 32

; in float fancy_attribute;
%6 = OpTypePointer Input %5
%7 = OpVariable %6 Input

%1 = OpTypeVoid
%2 = OpTypeFunction %1
%3 = OpFunction %1 None %2
%4 = OpLabel

     OpReturn
     OpFunctionEnd
